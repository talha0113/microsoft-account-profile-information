{
  "testURL": "http://localhost",
  "rootDir": "../../",
  "bail": true,
  "cache": false,
  "clearMocks": true,
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json",
    "html"
  ],
  "globals": {
    "ts-jest": {
      "tsConfig": "./tsconfig.json"
    }
  },
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(t|j)sx?$",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "globalSetup": "<rootDir>/Configurations/Jest/jest.setup.js",
  "globalTeardown": "<rootDir>/Configurations/Jest/jest.teardown.js",
  "testEnvironment": "<rootDir>/Configurations/Jest/jest.environment.js",
  "setupFilesAfterEnv": [ "<rootDir>/Configurations/Jest/jest.init.js" ],
  "testResultsProcessor": "<rootDir>/Configurations/Jest/jest.trx.processor"
}
